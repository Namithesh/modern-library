            <div class="main-content" style="padding-left:20px; padding-right:20px; padding-bottom:25px;">

    <div class="fluid-container p-4">
    <h1 class="display-3">Successful Orders</h1>

    <% if (data) { %>
        <div style="overflow-x:auto; padding:0px;">
            <table class="table table-striped table-light">
                <tbody>
                    <tr>
                        <th></th>
                        <th>ORDER ID</th>
                        <th>Order Status</th>
                        <th>Customer Name</th>
                        <th>Customer Phone</th>
                        <!-- <th>Address</th> -->
                        <th>Printing Charges</th>
                        <th>Other Charges</th>
                        <th>Total Amount</th>
                        <th>Payment</th>
                        <th>View Details</th>
                        <th>Delivery Method</th>
                        <th>Order Date</th>
                    </tr>
                    <% for (let i=0; i<orders.length; i++) { %>
                        <tr>
                            <td>
                                <%= i+1 %>
                            </td>
                            <td>
                                <%=orders[i].order_id%>
                            </td>
                            <td>
                                <%=orders[i].order_status%>
                            </td>
                            <td>
                                <%=orders[i].name%>
                            </td>
                            <td>
                                <%=orders[i].phone%>
                            </td>
                            <!-- <td>
                                cust-address
                            </td> -->
                            <td>
                                <%=orders[i].pvc_charge%>
                            </td>
                            <td>
                                <%=orders[i].delivery_charge + orders[i].tax_charge + orders[i].pay_charge%>
                            </td>
                            <td>
                                <%=orders[i].total_charge%>
                            </td>
                            <td align=" ">
                                <%=orders[i].payment_status%>
                            </td>
                            <td align="">
                                <a href="/dashboard/orders/<%=orders[i].order_id%>">Open</a>
                            </td>
                            <td>
                                <%=orders[i].delivery_method%>
                            </td>
                            <td><%=orders[i].order_date %></td>
                            <!-- <td><%=orders[i].formatted_date %></td> -->
                        </tr>
                            
                    <% } %>
                </tbody>
            </table>
        </div>
        <% } else { %>
        <div style="padding-bottom:200px;">
            No Orders.
            <hr>
        </div>
        <% } %>
    <!-- </div> -->

   
        <a href="/dashboard/allOrders/"><h1>All Orders</h1></a>
